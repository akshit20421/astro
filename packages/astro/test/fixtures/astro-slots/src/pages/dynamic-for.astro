---
import Slotted from '../components/Slotted.astro';

const slotNames = ['a', 'b', 'c']
---

<html>
  <head>
    <!-- Head Stuff -->
  </head>
  <body>
    <Slotted>
      {()=>{
        const slots:any[] = [];
        for (const slotName of slotNames) {
          slots.push(<span slot={slotName}>{slotName.toUpperCase()}</span>);
        }
        return slots;
      }
      }
    <span>Default</span>
    </Slotted>
  </body>
</html>
